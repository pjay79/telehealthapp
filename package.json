{
  "main": "index.js",
  "scripts": {
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/telehealthapp/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "bundle:android:js": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "android:dev": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android:qa": "react-native run-android --variant=qaDebug --appIdSuffix=qa",
    "android:prod": "react-native run-android --variant=prodDebug --appIdSuffix=prod",
    "android:dev:release": "yarn bundle:android:js && cd android && ./gradlew assembleDevRelease  && adb install -r app/build/outputs/apk/dev/release/app-dev-release.apk",
    "android:qa:release": "yarn bundle:android:js && cd android && ./gradlew assembleQaRelease  && adb install -r app/build/outputs/apk/qa/release/app-qa-release.apk",
    "android:prod:release": "yarn bundle:android:js && cd android && ./gradlew assembleProdRelease && adb install -r app/build/outputs/apk/prod/release/app-prod-release.apk",
    "web": "expo start --web",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.9",
    "@react-navigation/native": "^5.1.5",
    "expo": "~37.0.3",
    "expo-updates": "~0.1.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "~0.61.5",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-unimodules": "~0.9.0",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "babel-jest": "~25.2.6",
    "jest": "~25.2.6",
    "react-test-renderer": "~16.9.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "private": true
}
